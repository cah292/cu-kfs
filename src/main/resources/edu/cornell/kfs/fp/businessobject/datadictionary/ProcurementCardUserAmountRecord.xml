<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:p="http://www.springframework.org/schema/p" 
	   xsi:schemaLocation="http://www.springframework.org/schema/beans         
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProcurementCardUserAmountRecord" parent="ProcurementCardUserAmountRecord-parentBean"/>

  <bean id="ProcurementCardUserAmountRecord-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.fp.businessobject.ProcurementCardUserAmountRecord"/>
    <property name="objectLabel" value="Master Card Transaction Detail Addemdum 11"/>
    <property name="attributes">
      <list>
      	<ref bean="ProcurementCardUserAmountRecord-pcardUserAmountId"/>
        <ref bean="ProcurementCardUserAmountRecord-transactionSequenceRowNumber"/>
        <ref bean="ProcurementCardUserAmountRecord-userAmount"/>
      </list>
    </property>
  </bean>

  <bean id="ProcurementCardUserAmountRecord-pcardUserAmountId" parent="ProcurementCardUserAmountRecord-pcardUserAmountId-parentBean" />
  <bean id="ProcurementCardUserAmountRecord-pcardUserAmountId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="pcardUserAmountId" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="9" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
  <bean id="ProcurementCardUserAmountRecord-transactionSequenceRowNumber" parent="ProcurementCardUserAmountRecord-transactionSequenceRowNumber-parentBean" />
  <bean id="ProcurementCardUserAmountRecord-transactionSequenceRowNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionSequenceRowNumber" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="9" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
  <bean id="ProcurementCardUserAmountRecord-userAmount" parent="ProcurementCardUserAmountRecord-userAmount-parentBean" />
  <bean id="ProcurementCardUserAmountRecord-userAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="userAmount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
</beans>