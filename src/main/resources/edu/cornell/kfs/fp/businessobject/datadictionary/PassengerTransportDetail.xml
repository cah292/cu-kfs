<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:p="http://www.springframework.org/schema/p" 
	   xsi:schemaLocation="http://www.springframework.org/schema/beans         
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PassengerTransportDetail" parent="PassengerTransportDetail-parentBean"/>

  <bean id="PassengerTransportDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.fp.businessobject.PassengerTransportDetail"/>
    <property name="objectLabel" value="Passenger Transport Detail"/>
    <property name="attributes">
      <list>
      	<ref bean="PassengerTransportDetail-passengerTransportId"/>
        <ref bean="PassengerTransportDetail-documentNumber"/>
        <ref bean="PassengerTransportDetail-financialDocumentTransactionLineNumber"/>
        <ref bean="PassengerTransportDetail-passengerName"/>
        <ref bean="PassengerTransportDetail-departureDate"/>
        <ref bean="PassengerTransportDetail-airportCode"/>
        <ref bean="PassengerTransportDetail-travelAgencyCode"/>
        <ref bean="PassengerTransportDetail-travelAgencyName"/>
        <ref bean="PassengerTransportDetail-ticketNumber"/>
        <ref bean="PassengerTransportDetail-customerCode"/>
        <ref bean="PassengerTransportDetail-issueDate"/>
        <ref bean="PassengerTransportDetail-issuingCarrier"/>
        <ref bean="PassengerTransportDetail-totalFare"/>
        <ref bean="PassengerTransportDetail-totalFees"/>
        <ref bean="PassengerTransportDetail-totalTaxes"/>
      </list>
    </property>
  </bean>

  <bean id="PassengerTransportDetail-passengerTransportId" parent="PassengerTransportDetail-passengerTransportId-parentBean" />
  <bean id="PassengerTransportDetail-passengerTransportId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="passengerTransportId" />
    <property name="label" value="Passenger Transport Id" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="9" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-documentNumber" parent="PassengerTransportDetail-documentNumber-parentBean" />
  <bean id="PassengerTransportDetail-documentNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber">
    <property name="forceUppercase" value="false" />
  </bean>

  <bean id="PassengerTransportDetail-financialDocumentTransactionLineNumber" parent="PassengerTransportDetail-financialDocumentTransactionLineNumber-parentBean" />
  <bean id="PassengerTransportDetail-financialDocumentTransactionLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentTransactionLineNumber"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Financial Document Transaction Line Number"/>
    <property name="shortLabel" value="Number"/>
    <property name="maxLength" value="7"/>
    <property name="validationPattern">
      <ref bean="NumericValidation" />
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>

  <bean id="PassengerTransportDetail-passengerName" parent="PassengerTransportDetail-passengerName-parentBean" />
  <bean id="PassengerTransportDetail-passengerName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="passengerName" />
    <property name="label" value="Passenger Name" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="25" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="25"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-departureDate" parent="PassengerTransportDetail-departureDate-parentBean" />
  <bean id="PassengerTransportDetail-departureDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="departureDate" />
    <property name="label" value="Departure Date" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="8" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="8"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-airportCode" parent="PassengerTransportDetail-airportCode-parentBean" />
  <bean id="PassengerTransportDetail-airportCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="airportCode" />
    <property name="label" value="Airport Code" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="5" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="5"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-travelAgencyCode" parent="PassengerTransportDetail-travelAgencyCode-parentBean" />
  <bean id="PassengerTransportDetail-travelAgencyCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="travelAgencyCode" />
    <property name="label" value="Travel Agency Code" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="8" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="8"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-travelAgencyName" parent="PassengerTransportDetail-travelAgencyName-parentBean" />
  <bean id="PassengerTransportDetail-travelAgencyName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="travelAgencyName" />
    <property name="label" value="Travel Agency Name" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="25" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="25"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-ticketNumber" parent="PassengerTransportDetail-ticketNumber-parentBean" />
  <bean id="PassengerTransportDetail-ticketNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="ticketNumber" />
    <property name="label" value="Ticket Number" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="15" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="15"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-customerCode" parent="PassengerTransportDetail-customerCode-parentBean" />
  <bean id="PassengerTransportDetail-customerCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerCode" />
    <property name="label" value="Customer Code" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="64" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="64"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-issueDate" parent="PassengerTransportDetail-issueDate-parentBean" />
  <bean id="PassengerTransportDetail-issueDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="issueDate" />
    <property name="label" value="Issue Date" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="8" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="8"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-issuingCarrier" parent="PassengerTransportDetail-issuingCarrier-parentBean" />
  <bean id="PassengerTransportDetail-issuingCarrier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="issuingCarrier" />
    <property name="label" value="Issuing Carrier" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="4" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-totalFare" parent="PassengerTransportDetail-totalFare-parentBean" />
  <bean id="PassengerTransportDetail-totalFare-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="totalFare" />
    <property name="label" value="Total Fare" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-totalFees" parent="PassengerTransportDetail-totalFees-parentBean" />
  <bean id="PassengerTransportDetail-totalFees-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="totalFees" />
    <property name="label" value="Total Fees" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportDetail-totalTaxes" parent="PassengerTransportDetail-totalTaxes-parentBean" />
  <bean id="PassengerTransportDetail-totalTaxes-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="totalTaxes" />
    <property name="label" value="Total Taxes" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
</beans>