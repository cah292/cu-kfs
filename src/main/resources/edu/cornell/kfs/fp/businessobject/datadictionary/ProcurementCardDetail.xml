  <bean id="ProcurementCardDetail" parent="ProcurementCardDetail-parentBean"/>
  <bean id="ProcurementCardDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.fp.businessobject.ProcurementCardDetail"/>
    <property name="objectLabel" value="Procurement Card Detail"/>
    <property name="attributes">
      <list merge="true">
      	<ref bean="ProcurementCardDetail-pcardId"/>
        <ref bean="ProcurementCardDetail-documentNumber"/>
        <ref bean="ProcurementCardDetail-financialDocumentTransactionLineNumber"/>
        <ref bean="ProcurementCardDetail-productCode"/>
        <ref bean="ProcurementCardDetail-itemDescription"/>
        <ref bean="ProcurementCardDetail-itemQuantity"/>
        <ref bean="ProcurementCardDetail-itemUnitOfMeasure"/>
        <ref bean="ProcurementCardDetail-extendedItemAmount"/>
        <ref bean="ProcurementCardDetail-debitCreditInd"/>
        <ref bean="ProcurementCardDetail-netGrossIndicator"/>
        <ref bean="ProcurementCardDetail-taxRateApplied"/>
        <ref bean="ProcurementCardDetail-taxTypeApplied"/>
        <ref bean="ProcurementCardDetail-taxAmount"/>
        <ref bean="ProcurementCardDetail-discountIndicator"/>
        <ref bean="ProcurementCardDetail-discountAmount"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="ProcurementCardDetail-pcardId" parent="ProcurementCardDetail-pcardId-parentBean" />
  <bean id="ProcurementCardDetail-pcardId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="pcardId" />
    <property name="label" value="Pcard Id" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="9" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
  <bean id="ProcurementCardDetail-documentNumber" parent="ProcurementCardDetail-documentNumber-parentBean" />
  <bean id="ProcurementCardDetail-documentNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber">
    <property name="forceUppercase" value="false" />
  </bean>

  <bean id="ProcurementCardDetail-financialDocumentTransactionLineNumber" parent="ProcurementCardDetail-financialDocumentTransactionLineNumber-parentBean" />
  <bean id="ProcurementCardDetail-financialDocumentTransactionLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentTransactionLineNumber"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Financial Document Transaction Line Number"/>
    <property name="shortLabel" value="Number"/>
    <property name="maxLength" value="7"/>
    <property name="validationPattern">
      <ref bean="NumericValidation" />
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
  <bean id="ProcurementCardDetail-productCode" parent="ProcurementCardDetail-productCode-parentBean" />
  <bean id="ProcurementCardDetail-productCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="productCode" />
    <property name="label" value="Product Code" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="12" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="12"/>
    </property>
  </bean>
  
  <bean id="ProcurementCardDetail-itemDescription" parent="ProcurementCardDetail-itemDescription-parentBean" />
  <bean id="ProcurementCardDetail-itemDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemDescription" />
    <property name="label" value="Item Description" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="35" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="35"/>
    </property>
  </bean>

  <bean id="ProcurementCardDetail-itemQuantity" parent="ProcurementCardDetail-itemQuantity-parentBean" />
  <bean id="ProcurementCardDetail-itemQuantity-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemQuantity" />
    <property name="label" value="Item Quantity" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="5" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="5"/>
    </property>
  </bean>
 
  <bean id="ProcurementCardDetail-itemUnitOfMeasure" parent="ProcurementCardDetail-itemUnitOfMeasure-parentBean" />
  <bean id="ProcurementCardDetail-itemUnitOfMeasure-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemUnitOfMeasure" />
    <property name="label" value="Item Unit Of Measure" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="3" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
  </bean>
 
  <bean id="ProcurementCardDetail-extendedItemAmount" parent="ProcurementCardDetail-extendedItemAmount-parentBean" />
  <bean id="ProcurementCardDetail-extendedItemAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="extendedItemAmount" />
    <property name="label" value="Extended Item Amount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
 
  <bean id="ProcurementCardDetail-debitCreditInd" parent="ProcurementCardDetail-debitCreditInd-parentBean" />
  <bean id="ProcurementCardDetail-debitCreditInd-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="debitCreditInd" />
    <property name="label" value="Debit Credit Ind" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
 
  <bean id="ProcurementCardDetail-netGrossIndicator" parent="ProcurementCardDetail-netGrossIndicator-parentBean" />
  <bean id="ProcurementCardDetail-netGrossIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="netGrossIndicator" />
    <property name="label" value="Net Gross Indicator" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
 
  <bean id="ProcurementCardDetail-taxRateApplied" parent="ProcurementCardDetail-taxRateApplied-parentBean" />
  <bean id="ProcurementCardDetail-taxRateApplied-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxRateApplied" />
    <property name="label" value="Tax Rate Applied" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="5" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="5"/>
    </property>
  </bean>
 
  <bean id="ProcurementCardDetail-taxTypeApplied" parent="ProcurementCardDetail-taxTypeApplied-parentBean" />
  <bean id="ProcurementCardDetail-taxTypeApplied-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxTypeApplied" />
    <property name="label" value="Tax Type Applied" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="4" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
  </bean>
 
  <bean id="ProcurementCardDetail-taxAmount" parent="ProcurementCardDetail-taxAmount-parentBean" />
  <bean id="ProcurementCardDetail-taxAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxAmount" />
    <property name="label" value="Tax Amount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
 
  <bean id="ProcurementCardDetail-discountIndicator" parent="ProcurementCardDetail-discountIndicator-parentBean" />
  <bean id="ProcurementCardDetail-discountIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="discountIndicator" />
    <property name="label" value="Discount Indicator" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
 
  <bean id="ProcurementCardDetail-discountAmount" parent="ProcurementCardDetail-discountAmount-parentBean" />
  <bean id="ProcurementCardDetail-discountAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="discountAmount" />
    <property name="label" value="Discount Amount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>

</beans>
