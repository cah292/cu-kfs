<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:p="http://www.springframework.org/schema/p" 
	   xsi:schemaLocation="http://www.springframework.org/schema/beans         
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="MasterCardTransactionDetailAddendum11" parent="MasterCardTransactionDetailAddendum11-parentBean"/>

  <bean id="MasterCardTransactionDetailAddendum11-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.fp.businessobject.MasterCardTransactionDetailAddendum11"/>
    <property name="objectLabel" value="Master Card Transaction Detail Addemdum 11"/>
    <property name="attributes">
      <list>
        <ref bean="TransactionDetailAddendum11-recordType"/>
        <ref bean="TransactionDetailAddendum11-filler1"/>
        <ref bean="TransactionDetailAddendum11-addendumType"/>
        <ref bean="TransactionDetailAddendum11-filler2"/>
        <ref bean="TransactionDetailAddendum11-userAmount"/>
        <ref bean="TransactionDetailAddendum11-filler3"/>
      </list>
    </property>
  </bean>

  <bean id="TransactionDetailAddendum11-recordType" parent="TransactionDetailAddendum11-recordType-parentBean" />
  <bean id="TransactionDetailAddendum11-recordType-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="recordType" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="4" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
  </bean>

  <bean id="TransactionDetailAddendum11-filler1" parent="TransactionDetailAddendum11-filler1-parentBean" />
  <bean id="TransactionDetailAddendum11-filler1-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler1" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="35" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="35"/>
    </property>
  </bean>

  <bean id="TransactionDetailAddendum11-addendumType" parent="TransactionDetailAddendum11-addendumType-parentBean" />
  <bean id="TransactionDetailAddendum11-addendumType-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="addendumType" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="3" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
  </bean>

  <bean id="TransactionDetailAddendum11-filler2" parent="TransactionDetailAddendum11-filler2-parentBean" />
  <bean id="TransactionDetailAddendum11-filler2-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler2" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="23" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="23"/>
    </property>
  </bean>

  <bean id="TransactionDetailAddendum11-userAmount" parent="TransactionDetailAddendum11-userAmount-parentBean" />
  <bean id="TransactionDetailAddendum11-userAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="userAmount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="TransactionDetailAddendum11-filler3" parent="TransactionDetailAddendum11-filler3-parentBean" />
  <bean id="TransactionDetailAddendum11-filler3-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler3" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="15" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="15"/>
    </property>
  </bean>
  
</beans>