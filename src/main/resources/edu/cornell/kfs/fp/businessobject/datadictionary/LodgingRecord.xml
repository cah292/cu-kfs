<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:p="http://www.springframework.org/schema/p" 
	   xsi:schemaLocation="http://www.springframework.org/schema/beans         
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="LodgingRecord" parent="LodgingRecord-parentBean"/>

  <bean id="LodgingRecord-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.fp.businessobject.LodgingRecord"/>
    <property name="objectLabel" value="Lodging Record"/>
    <property name="attributes">
      <list>
     	<ref bean="LodgingRecord-lodgingId"/>
        <ref bean="LodgingRecord-transactionSequenceRowNumber"/>
        <ref bean="LodgingRecord-arrivalDate"/>
        <ref bean="LodgingRecord-departureDate"/>
        <ref bean="LodgingRecord-folioNumber"/>
        <ref bean="LodgingRecord-propertyPhoneNumber"/>
        <ref bean="LodgingRecord-customerServiceNumber"/>
        <ref bean="LodgingRecord-roomRate"/>
        <ref bean="LodgingRecord-roomTax"/>
        <ref bean="LodgingRecord-programCode"/>
        <ref bean="LodgingRecord-telephoneCharges"/>
        <ref bean="LodgingRecord-roomService"/>
        <ref bean="LodgingRecord-barCharges"/>
        <ref bean="LodgingRecord-giftShopCharges"/>
        <ref bean="LodgingRecord-laundryCharges"/>
        <ref bean="LodgingRecord-otherServicesCode"/>
        <ref bean="LodgingRecord-otherServicesCharges"/>
        <ref bean="LodgingRecord-billingAdjustmentIndicator"/>
        <ref bean="LodgingRecord-billingAdjustmentAmount"/>
      </list>
    </property>
  </bean>

  <bean id="LodgingRecord-lodgingId" parent="LodgingRecord-lodgingId-parentBean" />
  <bean id="LodgingRecord-lodgingId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="lodgingId" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="9" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
