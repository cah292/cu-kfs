  </bean>
  
  <bean id="NonFuelRecord-extendedItemAmount" parent="NonFuelRecord-extendedItemAmount-parentBean" />
  <bean id="NonFuelRecord-extendedItemAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="extendedItemAmount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-discountIndicator" parent="NonFuelRecord-discountIndicator-parentBean" />
  <bean id="NonFuelRecord-discountIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="discountIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-discountAmount" parent="NonFuelRecord-discountAmount-parentBean" />
  <bean id="NonFuelRecord-discountAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="discountAmount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-netGrossIndicator" parent="NonFuelRecord-netGrossIndicator-parentBean" />
  <bean id="NonFuelRecord-netGrossIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="netGrossIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-taxRateApplied" parent="NonFuelRecord-taxRateApplied-parentBean" />
  <bean id="NonFuelRecord-taxRateApplied-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxRateApplied" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="5" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="5"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-taxTypeApplied" parent="NonFuelRecord-taxTypeApplied-parentBean" />
  <bean id="NonFuelRecord-taxTypeApplied-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxTypeApplied" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="4" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-taxAmount" parent="NonFuelRecord-taxAmount-parentBean" />
  <bean id="NonFuelRecord-taxAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxAmount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-debitCreditIndicator" parent="NonFuelRecord-debitCreditIndicator-parentBean" />
  <bean id="NonFuelRecord-debitCreditIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="debitCreditInd" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-alternateTaxIdentifier" parent="NonFuelRecord-alternateTaxIdentifier-parentBean" />
  <bean id="NonFuelRecord-alternateTaxIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="alternateTaxIdentifier" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="15" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="15"/>
    </property>
  </bean>
  
</beans>