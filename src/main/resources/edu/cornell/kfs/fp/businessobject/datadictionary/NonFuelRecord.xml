<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:p="http://www.springframework.org/schema/p" 
	   xsi:schemaLocation="http://www.springframework.org/schema/beans         
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonFuelRecord" parent="NonFuelRecord-parentBean"/>

  <bean id="NonFuelRecord-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.fp.businessobject.NonFuelRecord"/>
    <property name="objectLabel" value="Non Fuel Record"/>
    <property name="attributes">
      <list>
      	<ref bean="NonFuelRecord-nonFuelId"/>
        <ref bean="NonFuelRecord-transactionSequenceRowNumber"/>
        <ref bean="NonFuelRecord-itemProductCode"/>
        <ref bean="NonFuelRecord-itemDescription"/>
        <ref bean="NonFuelRecord-itemQuantity"/>
        <ref bean="NonFuelRecord-itemUnitOfMeasure"/>
        <ref bean="NonFuelRecord-extendedItemAmount"/>
        <ref bean="NonFuelRecord-discountIndicator"/>
        <ref bean="NonFuelRecord-discountAmount"/>
        <ref bean="NonFuelRecord-netGrossIndicator"/>
        <ref bean="NonFuelRecord-taxRateApplied"/>
        <ref bean="NonFuelRecord-taxTypeApplied"/>
        <ref bean="NonFuelRecord-taxAmount"/>
        <ref bean="NonFuelRecord-debitCreditIndicator"/>
        <ref bean="NonFuelRecord-alternateTaxIdentifier"/>
      </list>
    </property>
  </bean>

  <bean id="NonFuelRecord-nonFuelId" parent="NonFuelRecord-nonFuelId-parentBean" />
  <bean id="NonFuelRecord-nonFuelId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="nonFuelId" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="9" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-transactionSequenceRowNumber" parent="NonFuelRecord-transactionSequenceRowNumber-parentBean" />
  <bean id="NonFuelRecord-transactionSequenceRowNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionSequenceRowNumber" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="9" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-itemProductCode" parent="NonFuelRecord-itemProductCode-parentBean" />
  <bean id="NonFuelRecord-itemProductCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemProductCode" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="12" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="12"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-itemDescription" parent="NonFuelRecord-itemDescription-parentBean" />
  <bean id="NonFuelRecord-itemDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemDescription" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="35" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="35"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-itemQuantity" parent="NonFuelRecord-itemQuantity-parentBean" />
  <bean id="NonFuelRecord-itemQuantity-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemQuantity" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="5" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="5"/>
    </property>
  </bean>
  
  <bean id="NonFuelRecord-itemUnitOfMeasure" parent="NonFuelRecord-itemUnitOfMeasure-parentBean" />
  <bean id="NonFuelRecord-itemUnitOfMeasure-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemUnitOfMeasure" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="3" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
