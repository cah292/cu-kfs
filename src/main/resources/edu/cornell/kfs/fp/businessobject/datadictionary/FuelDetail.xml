<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:p="http://www.springframework.org/schema/p" 
	   xsi:schemaLocation="http://www.springframework.org/schema/beans         
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FuelDetail" parent="FuelDetail-parentBean"/>

  <bean id="FuelDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.fp.businessobject.FuelDetail"/>
    <property name="objectLabel" value="Fuel Detail"/>
    <property name="attributes">
      <list>
      	<ref bean="FuelDetail-fuelId"/>
        <ref bean="FuelDetail-documentNumber"/>
        <ref bean="FuelDetail-financialDocumentTransactionLineNumber"/>
        <ref bean="FuelDetail-oilCompanyBrand"/>
        <ref bean="FuelDetail-merchantStreetAddress"/>
        <ref bean="FuelDetail-merchantPostalCode"/>
        <ref bean="FuelDetail-timeOfPurchase"/>
        <ref bean="FuelDetail-motorFuelServiceType"/>
        <ref bean="FuelDetail-motorFuelProductCode"/>
        <ref bean="FuelDetail-motorFuelUnitPrice"/>
        <ref bean="FuelDetail-motorFuelUnitOfMeasure"/>
        <ref bean="FuelDetail-motorFuelQuantity"/>
        <ref bean="FuelDetail-motorFuelSaleAmount"/>
        <ref bean="FuelDetail-odomoterReading"/>
        <ref bean="FuelDetail-vehicleNumber"/>
        <ref bean="FuelDetail-driverNumber"/>
        <ref bean="FuelDetail-magneticStripeProductTypeCode"/>
        <ref bean="FuelDetail-couponDiscountAmount"/>
        <ref bean="FuelDetail-taxExemptAmount"/>
        <ref bean="FuelDetail-taxAmount1"/>
        <ref bean="FuelDetail-taxAmount2"/>
      </list>
    </property>
  </bean>

  <bean id="FuelDetail-fuelId" parent="FuelDetail-fuelId-parentBean" />
  <bean id="FuelDetail-fuelId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="fuelId" />
    <property name="label" value="Fuel Id" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="9" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-documentNumber" parent="FuelDetail-documentNumber-parentBean" />
  <bean id="FuelDetail-documentNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber">
    <property name="forceUppercase" value="false" />
  </bean>

  <bean id="FuelDetail-financialDocumentTransactionLineNumber" parent="FuelDetail-financialDocumentTransactionLineNumber-parentBean" />
  <bean id="FuelDetail-financialDocumentTransactionLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentTransactionLineNumber"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Financial Document Transaction Line Number"/>
    <property name="shortLabel" value="Number"/>
    <property name="maxLength" value="7"/>
    <property name="validationPattern">
      <ref bean="NumericValidation" />
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-oilCompanyBrand" parent="FuelDetail-oilCompanyBrand-parentBean" />
  <bean id="FuelDetail-oilCompanyBrand-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="oilCompanyBrand" />
    <property name="label" value="Oil Company Brand" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="4" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-merchantStreetAddress" parent="FuelDetail-merchantStreetAddress-parentBean" />
  <bean id="FuelDetail-merchantStreetAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="merchantStreetAddress" />
    <property name="label" value="Merchant Street Address" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="35" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="35"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-merchantPostalCode" parent="FuelDetail-merchantPostalCode-parentBean" />
  <bean id="FuelDetail-merchantPostalCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="merchantPostalCode" />
    <property name="label" value="Merchant Postal Code" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="10" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-timeOfPurchase" parent="FuelDetail-timeOfPurchase-parentBean" />
  <bean id="FuelDetail-timeOfPurchase-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="timeOfPurchase" />
    <property name="label" value="Time Of Purchase" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="4" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-motorFuelServiceType" parent="FuelDetail-motorFuelServiceType-parentBean" />
  <bean id="FuelDetail-motorFuelServiceType-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="motorFuelServiceType" />
    <property name="label" value="Motor Fuel Service Type" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-motorFuelProductCode" parent="FuelDetail-motorFuelProductCode-parentBean" />
  <bean id="FuelDetail-motorFuelProductCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="motorFuelProductCode" />
    <property name="label" value="Motor Fuel Product Code" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="3" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-motorFuelUnitPrice" parent="FuelDetail-motorFuelUnitPrice-parentBean" />
  <bean id="FuelDetail-motorFuelUnitPrice-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="motorFuelUnitPrice" />
    <property name="label" value="Motor Fuel Unit Price" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-motorFuelUnitOfMeasure" parent="FuelDetail-motorFuelUnitOfMeasure-parentBean" />
  <bean id="FuelDetail-motorFuelUnitOfMeasure-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="motorFuelUnitOfMeasure" />
    <property name="label" value="Motor Fuel Unit Of Measure" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-motorFuelQuantity" parent="FuelDetail-motorFuelQuantity-parentBean" />
  <bean id="FuelDetail-motorFuelQuantity-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="motorFuelQuantity" />
    <property name="label" value="Motor Fuel Quantity" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-motorFuelSaleAmount" parent="FuelDetail-motorFuelSaleAmount-parentBean" />
  <bean id="FuelDetail-motorFuelSaleAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="motorFuelSaleAmount" />
    <property name="label" value="Motor Fuel Sale Amount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-odomoterReading" parent="FuelDetail-odomoterReading-parentBean" />
  <bean id="FuelDetail-odomoterReading-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="odomoterReading" />
    <property name="label" value="Odometer Reading" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="7" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="7"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-vehicleNumber" parent="FuelDetail-vehicleNumber-parentBean" />
  <bean id="FuelDetail-vehicleNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vehicleNumber" />
    <property name="label" value="Vehicle Number" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="17" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="17"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-driverNumber" parent="FuelDetail-driverNumber-parentBean" />
  <bean id="FuelDetail-driverNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="driverNumber" />
    <property name="label" value="Driver Number" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="17" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="17"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-magneticStripeProductTypeCode" parent="FuelDetail-magneticStripeProductTypeCode-parentBean" />
  <bean id="FuelDetail-magneticStripeProductTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="magneticStripeProductTypeCode" />
    <property name="label" value="Magnetic Stripe Product Type Code" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-couponDiscountAmount" parent="FuelDetail-couponDiscountAmount-parentBean" />
  <bean id="FuelDetail-couponDiscountAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="couponDiscountAmount" />
    <property name="label" value="Coupon Discount Amount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-taxExemptAmount" parent="FuelDetail-taxExemptAmount-parentBean" />
  <bean id="FuelDetail-taxExemptAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxExemptAmount" />
    <property name="label" value="Tax Exempt Amount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-taxAmount1" parent="FuelDetail-taxAmount1-parentBean" />
  <bean id="FuelDetail-taxAmount1-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxAmount1" />
    <property name="label" value="Tax Amount 1" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="FuelDetail-taxAmount2" parent="FuelDetail-taxAmount2-parentBean" />
  <bean id="FuelDetail-taxAmount2-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxAmount2" />
    <property name="label" value="Tax Amount 2" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
</beans>