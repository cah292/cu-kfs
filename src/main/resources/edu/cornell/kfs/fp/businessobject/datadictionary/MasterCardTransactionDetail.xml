<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:p="http://www.springframework.org/schema/p" 
	   xsi:schemaLocation="http://www.springframework.org/schema/beans         
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="MasterCardTransactionDetail" parent="MasterCardTransactionDetail-parentBean"/>

  <bean id="MasterCardTransactionDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.fp.businessobject.MasterCardTransactionDetail"/>
    <property name="objectLabel" value="Master Card Transaction Detail"/>
    <property name="attributes">
      <list>
        <ref bean="TransactionDetail-recordType"/>
        <ref bean="TransactionDetail-filler1"/>
        <ref bean="TransactionDetail-addendumType"/>
        <ref bean="TransactionDetail-filler2"/>
        <ref bean="TransactionDetail-creditCardNumber"/>
        <ref bean="TransactionDetail-filler3"/>
        <ref bean="TransactionDetail-transactionRefNumber"/>
        <ref bean="TransactionDetail-filler4"/>
        <ref bean="TransactionDetail-debitCreditIndicator"/>
        <ref bean="TransactionDetail-transactionAmount"/>
        <ref bean="TransactionDetail-postingDate"/>
        <ref bean="TransactionDetail-transactionDate"/>
        <ref bean="TransactionDetail-filler5"/>
        <ref bean="TransactionDetail-merchantName"/>
        <ref bean="TransactionDetail-merchantCity"/>
        <ref bean="TransactionDetail-merchantState"/>
        <ref bean="TransactionDetail-merchantCountry"/>
        <ref bean="TransactionDetail-categoryCode"/>
        <ref bean="TransactionDetail-origCurrencyAmount"/>
        <ref bean="TransactionDetail-origCurrencyCode"/>
        <ref bean="TransactionDetail-filler6"/>
        <ref bean="TransactionDetail-salesTaxAmount"/>
        <ref bean="TransactionDetail-customerCode"/>
        <ref bean="TransactionDetail-filler7"/>
      </list>
    </property>
  </bean>

  <bean id="TransactionDetail-recordType" parent="TransactionDetail-recordType-parentBean" />
  <bean id="TransactionDetail-recordType-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="recordType" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="4" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
  </bean>

  <bean id="TransactionDetail-filler1" parent="CTransactionDetail-filler1-parentBean" />
  <bean id="CTransactionDetail-filler1-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler1" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="35" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="35"/>
    </property>
  </bean>

  <bean id="TransactionDetail-addendumType" parent="TransactionDetail-addendumType-parentBean" />
  <bean id="TransactionDetail-addendumType-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="addendumType" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="3" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
  </bean>

  <bean id="TransactionDetail-filler2" parent="TransactionDetail-filler2-parentBean" />
  <bean id="TransactionDetail-filler2-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler2" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="24" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="24"/>
    </property>
  </bean>

  <bean id="TransactionDetail-creditCardNumber" parent="TransactionDetail-creditCardNumber-parentBean" />
  <bean id="TransactionDetail-creditCardNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="creditCardNumber" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="TransactionDetail-filler3" parent="TransactionDetail-filler3-parentBean" />
  <bean id="TransactionDetail-filler3-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler3" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="15" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="15"/>
    </property>
  </bean>
  
  <bean id="TransactionDetail-transactionRefNumber" parent="TransactionDetail-transactionRefNumber-parentBean" />
  <bean id="TransactionDetail-transactionRefNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionRefNumber" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="21" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="21"/>
    </property>
  </bean>
  
  <bean id="TransactionDetail-filler4" parent="TransactionDetail-filler4-parentBean" />
  <bean id="TransactionDetail-filler4-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler4" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="2" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="2"/>
    </property>
  </bean>
  
  <bean id="TransactionDetail-debitCreditIndicator" parent="TransactionDetail-debitCreditIndicator-parentBean" />
  <bean id="TransactionDetail-debitCreditIndicator-parentBean" abstract="true" parent="GenericAttributes-oneDigitTextCode">
    <property name="name" value="debitCreditInd" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
  </bean>

  <bean id="TransactionDetail-transactionAmount" parent="TransactionDetail-transactionAmount-parentBean" />
  <bean id="TransactionDetail-transactionAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionAmount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>

  <bean id="TransactionDetail-postingDate" parent="TransactionDetail-postingDate-parentBean" />
  <bean id="TransactionDetail-postingDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="postingDate" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="8" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="8"/>
    </property>
  </bean>

  <bean id="TransactionDetail-transactionDate" parent="TransactionDetail-transactionDate-parentBean" />
  <bean id="TransactionDetail-transactionDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionDate" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="8" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="8"/>
    </property>
  </bean>

  <bean id="TransactionDetail-filler5" parent="TransactionDetail-filler5-parentBean" />
  <bean id="TransactionDetail-filler5-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler5" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="8" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="8"/>
    </property>
  </bean>
  
  <bean id="TransactionDetail-merchantName" parent="TransactionDetail-merchantName-parentBean" />
  <bean id="TransactionDetail-merchantName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="merchantName" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="25" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="25"/>
    </property>
  </bean>

  <bean id="TransactionDetail-merchantCity" parent="TransactionDetail-merchantCity-parentBean" />
  <bean id="TransactionDetail-merchantCity-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="merchantCity" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="13" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="13"/>
    </property>
  </bean>
  
  <bean id="TransactionDetail-merchantState" parent="TransactionDetail-merchantState-parentBean" />
  <bean id="TransactionDetail-merchantState-parentBean" abstract="true" parent="GenericAttributes-twoDigitTextCode">
    <property name="name" value="merchantState" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="2" />
  </bean>
  
  <bean id="TransactionDetail-merchantCountry" parent="TransactionDetail-merchantCountry-parentBean" />
  <bean id="TransactionDetail-merchantCountry-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="merchantCountry" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="4" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
