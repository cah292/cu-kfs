  <bean id="TransactionDetail-origCurrencyAmount" parent="TransactionDetail-origCurrencyAmount-parentBean" />
  <bean id="TransactionDetail-origCurrencyAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="origCurrencyAmount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>

  <bean id="TransactionDetail-origCurrencyCode" parent="TransactionDetail-origCurrencyCode-parentBean" />
  <bean id="TransactionDetail-origCurrencyCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="origCurrencyCode" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="3" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
  </bean>

  <bean id="TransactionDetail-filler6" parent="TransactionDetail-filler6-parentBean" />
  <bean id="TransactionDetail-filler6-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler6" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="29" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="29"/>
