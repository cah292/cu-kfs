  </bean>
  
  <bean id="TransactionDetail-categoryCode" parent="TransactionDetail-categoryCode-parentBean" />
  <bean id="TransactionDetail-categoryCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="categoryCode" />
