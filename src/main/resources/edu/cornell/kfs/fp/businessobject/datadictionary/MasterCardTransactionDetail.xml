  <bean id="TransactionDetail-customerCode" parent="TransactionDetail-customerCode-parentBean" />
  <bean id="TransactionDetail-customerCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerCode" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="17" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="17"/>
    </property>
  </bean>

  <bean id="TransactionDetail-salesTaxAmount" parent="TransactionDetail-salesTaxAmount-parentBean" />
  <bean id="TransactionDetail-salesTaxAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="salesTaxAmount" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>

  <bean id="TransactionDetail-filler7" parent="TransactionDetail-filler7-parentBean" />
  <bean id="TransactionDetail-filler7-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="filler7" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
 
</beans>
