    <property name="objectLabel" value="Rental Car Record"/>
    <property name="attributes">
      <list>
      	<ref bean="RentalCarRecord-rentalCarId"/>
        <ref bean="RentalCarRecord-transactionSequenceRowNumber"/>
        <ref bean="RentalCarRecord-rentalAgreementNumber"/>
        <ref bean="RentalCarRecord-renterName"/>
        <ref bean="RentalCarRecord-rentalReturnCity"/>
        <ref bean="RentalCarRecord-rentalReturnState"/>
        <ref bean="RentalCarRecord-rentalReturnCountry"/>
