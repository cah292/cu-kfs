<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:p="http://www.springframework.org/schema/p" 
	   xsi:schemaLocation="http://www.springframework.org/schema/beans         
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FuelRecord" parent="FuelRecord-parentBean"/>

  <bean id="FuelRecord-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.fp.businessobject.FuelRecord"/>
    <property name="objectLabel" value="Fuel Record"/>
    <property name="attributes">
      <list>
      	<ref bean="FuelRecord-fuelId"/>
        <ref bean="FuelRecord-transactionSequenceRowNumber"/>
        <ref bean="FuelRecord-oilCompanyBrand"/>
        <ref bean="FuelRecord-merchantStreetAddress"/>
        <ref bean="FuelRecord-merchantPostalCode"/>
        <ref bean="FuelRecord-timeOfPurchase"/>
        <ref bean="FuelRecord-motorFuelServiceType"/>
        <ref bean="FuelRecord-motorFuelProductCode"/>
        <ref bean="FuelRecord-motorFuelUnitPrice"/>
        <ref bean="FuelRecord-motorFuelUnitOfMeasure"/>
        <ref bean="FuelRecord-motorFuelQuantity"/>
        <ref bean="FuelRecord-motorFuelSaleAmount"/>
        <ref bean="FuelRecord-odomoterReading"/>
        <ref bean="FuelRecord-vehicleNumber"/>
        <ref bean="FuelRecord-driverNumber"/>
        <ref bean="FuelRecord-magneticStripeProductTypeCode"/>
        <ref bean="FuelRecord-couponDiscountAmount"/>
        <ref bean="FuelRecord-taxExemptAmount"/>
        <ref bean="FuelRecord-taxAmount1"/>
        <ref bean="FuelRecord-taxAmount2"/>
      </list>
    </property>
  </bean>

  <bean id="FuelRecord-fuelId" parent="FuelRecord-fuelId-parentBean" />
  <bean id="FuelRecord-fuelId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="fuelId" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="9" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
  </bean>
  
