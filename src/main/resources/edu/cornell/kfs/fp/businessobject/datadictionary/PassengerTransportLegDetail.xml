    <property name="label" value="Arrival Time" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="4" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportLegDetail-arrivalTimeSegment" parent="PassengerTransportLegDetail-arrivalTimeSegment-parentBean" />
  <bean id="PassengerTransportLegDetail-arrivalTimeSegment-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="arrivalTimeSegment" />
    <property name="label" value="Arrival Time Segment" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="1" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="1"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportLegDetail-fare" parent="PassengerTransportLegDetail-fare-parentBean" />
  <bean id="PassengerTransportLegDetail-fare-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="fare" />
    <property name="label" value="Fare" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportLegDetail-fee" parent="PassengerTransportLegDetail-fee-parentBean" />
  <bean id="PassengerTransportLegDetail-fee-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="fee" />
    <property name="label" value="Fee" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="26"/>
