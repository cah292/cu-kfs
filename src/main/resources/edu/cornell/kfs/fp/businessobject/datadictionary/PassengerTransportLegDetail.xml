  <bean id="PassengerTransportLegDetail-taxes" parent="PassengerTransportLegDetail-taxes-parentBean" />
  <bean id="PassengerTransportLegDetail-taxes-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxes" />
    <property name="label" value="Taxes" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="16" />
    <property name="control">
      <bean parent="TextControlDefinition" p:size="16"/>
    </property>
  </bean>
  
  <bean id="PassengerTransportLegDetail-endorsements" parent="PassengerTransportLegDetail-endorsements-parentBean" />
  <bean id="PassengerTransportLegDetail-endorsements-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="endorsements" />
    <property name="label" value="Endorsements" />
    <property name="forceUppercase" value="false" />
    <property name="maxLength" value="20" />
    <property name="control">
