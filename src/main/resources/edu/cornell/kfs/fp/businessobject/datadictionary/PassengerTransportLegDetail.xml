  <bean id="PassengerTransportLegDetail-arrivalTime" parent="PassengerTransportLegDetail-arrivalTime-parentBean" />
  <bean id="PassengerTransportLegDetail-arrivalTime-parentBean" abstract="true" parent="AttributeDefinition">
