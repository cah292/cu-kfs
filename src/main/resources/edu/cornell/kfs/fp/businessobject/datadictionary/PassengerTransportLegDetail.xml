  </bean>
  
  <bean id="PassengerTransportLegDetail-departureTimeSegment" parent="PassengerTransportLegDetail-departureTimeSegment-parentBean" />
  <bean id="PassengerTransportLegDetail-departureTimeSegment-parentBean" abstract="true" parent="AttributeDefinition">
