        <ref bean="CardHolderDetail-recordType"/>
        <ref bean="CardHolderDetail-filler1"/>
        <ref bean="CardHolderDetail-creditCardNumber"/>
        <ref bean="CardHolderDetail-filler2"/>
        <ref bean="CardHolderDetail-cardHolderName"/>
        <ref bean="CardHolderDetail-cardHolderAltName"/>
