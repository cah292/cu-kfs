<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                          http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PRJESetMaintenanceDocument" 
        parent="PRJESetMaintenanceDocument-parentBean" />

  <bean id="PRJESetMaintenanceDocument-parentBean" abstract="true" 
        parent="MaintenanceDocumentEntry">
        
    <property name="businessObjectClass" 
              value="com.rsmart.kuali.kfs.prje.businessobject.PRJESet" />
    <property name="maintainableClass" 
              value="org.kuali.kfs.sys.document.FinancialSystemMaintainable" />
     
    <property name="maintainableSections" >
      <list>
        <ref bean="PRJESetMaintenanceDocument-EditSetPRJESetMaintenanceDocument-EditSet" />
      </list>
    </property>
    
    <property name="allowsNewOrCopy" value="true" />
    <property name="lockingKeys" >
      <list>
        <value>setId</value>
      </list>
    </property>

    <property name="documentAuthorizerClass" 
              value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeName" value="PRJS" />
  </bean>

  <!-- Maintenance Section Definition -->

  <bean id="PRJESetMaintenanceDocument-EditSetPRJESetMaintenanceDocument-EditSet" 
        parent="PRJESetMaintenanceDocument-EditSetPRJESetMaintenanceDocument-EditSet-parentBean" />

  <bean id="PRJESetMaintenanceDocument-EditSetPRJESetMaintenanceDocument-EditSet-parentBean" abstract="true" 
        parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Automated Journal Vouchers Set" />
    <property name="title" value="Edit Automated Journal Vouchers Set" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="setId" p:required="false"
              p:unconditionallyReadOnly="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="fiscalYear" p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="setName" p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="setDescription" p:required="false" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" p:required="false" />              
      </list>
    </property>
  </bean>
</beans>
